<% if current_user.likes?(@tweet) %>
  <%= link_to "Unlike", tweet_likes_path(@tweet), method: :delete %>
<% else %>
    <%= link_to "Like", tweet_likes_path(@tweet), method: :post %>
<% end %>

<%= @tweet.likes.count %>

Liked by:
<% @tweet.likes.each do |like| %>
  <br>
  <%= like.user.username %>
  <br>
<% end %>